{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FixMyTex",
  "version": "2.0.0",
  "identifier": "dev.michael-martin.FixMyTex",
  "build": {
    "frontendDist": "../../frontend/dist/frontend/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "cd ../frontend && ng serve",
    "beforeBuildCommand": "cd ../frontend && ng build"
  },
  "app": {
    "windows": [
      {
        "title": "FixMyTex",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/icons8-mutig-ai-32.png",
      "icons/icons8-mutig-ai-32.ico"
    ]
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFFNjdCODRCNTNBQjM1Q0MKUldUTU5hdFRTN2huSGxCMmJOdTZzL01FQjNVREQ3b2RFSmJlS1QvR3NjRHExeTkyd0dsKy9BeVYK",
      "endpoints": [
        "https://github.com/0xMMA/FixMyTex/releases/latest/download/latest.json"
      ]
    }
    }
  }
